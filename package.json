{
  "name": "universal-nuxt-boilerplate",
  "version": "1.0.0",
  "description": "My marvelous Nuxt.js project",
  "author": "ChangJoo Park",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "sql:db:migrate": "npx sequelize-cli db:migrate",
    "sql:db:migrate:schema:timestamps:add": "npx sequelize-cli db:migrate:schema:timestamps:add",
    "sql:db:migrate:status": "npx sequelize-cli db:migrate:status",
    "sql:db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "sql:db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "sql:db:seed": "npx sequelize-cli db:seed",
    "sql:db:seed:undo": "npx sequelize-cli db:seed:undo",
    "sql:db:seed:all": "npx sequelize-cli db:seed:all",
    "sql:db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "sql:db:create": "npx sequelize-cli db:create",
    "sql:db:drop": "npx sequelize-cli db:drop",
    "sql:init": "npx sequelize-cli init",
    "sql:init:config": "npx sequelize-cli init:config",
    "sql:init:migrations": "npx sequelize-cli init:migrations",
    "sql:init:models": "npx sequelize-cli init:models",
    "sql:init:seeders": "npx sequelize-cli init:seeders",
    "sql:migration:generate": "npx sequelize-cli migration:generate",
    "sql:model:generate": "npx sequelize-cli model:generate",
    "sql:seed:generate": "npx sequelize-cli seed:generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.4.0",
    "sequelize": "^5.8.6",
    "sqlite3": "^4.0.8"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^8.6.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "tailwindcss": "^1.0.0",
    "vue-jest": "^3.0.3"
  }
}
