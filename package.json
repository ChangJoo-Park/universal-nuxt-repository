{
  "name": "universal-nuxt-boilerplate",
  "version": "1.0.0",
  "description": "My marvelous Nuxt.js project",
  "author": "ChangJoo Park",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "storybook": "start-storybook",
    "sql:db:migrate": "sequelize db:migrate",
    "sql:db:migrate:schema:timestamps:add": "sequelize db:migrate:schema:timestamps:add",
    "sql:db:migrate:status": "sequelize db:migrate:status",
    "sql:db:migrate:undo": "sequelize db:migrate:undo",
    "sql:db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "sql:db:seed": "sequelize db:seed",
    "sql:db:seed:undo": "sequelize db:seed:undo",
    "sql:db:seed:all": "sequelize db:seed:all",
    "sql:db:seed:undo:all": "sequelize db:seed:undo:all",
    "sql:db:create": "sequelize db:create",
    "sql:db:drop": "sequelize db:drop",
    "sql:init": "sequelize init",
    "sql:init:config": "sequelize init:config",
    "sql:init:migrations": "sequelize init:migrations",
    "sql:init:models": "sequelize init:models",
    "sql:init:seeders": "sequelize init:seeders",
    "sql:migration:generate": "sequelize migration:generate",
    "sql:model:generate": "sequelize model:generate",
    "sql:seed:generate": "sequelize seed:generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/markdownit": "^1.2.4",
    "@nuxtjs/pwa": "^2.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "codemirror": "^5.46.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "highlight.js": "^9.15.6",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.8.6",
    "sqlite3": "^4.0.8"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@storybook/vue": "^5.0.11",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^8.6.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "faker": "^4.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "tailwindcss": "^1.0.0",
    "vue-jest": "^3.0.3"
  }
}
